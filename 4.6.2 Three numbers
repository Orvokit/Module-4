//Напишите программу, которая запрашивает у пользователя три числа и сообщает, есть ли среди них число больше пяти.

package main

import (
	"fmt"
)

func main() {
  var number1, number2, number3 int
	fmt.Print("Введите первое число: ")
  fmt.Scan(&number1)
  fmt.Print("Введите второе число: ")
  fmt.Scan(&number2)
  fmt.Print("Введите третье число: ")
  fmt.Scan(&number3)

  if number1 > 5 || number2 > 5 || number3 > 5 {
    fmt.Println("Среди введённых чисел есть число больше 5.")
  } else {
    fmt.Println("Среди введённых чисел нет чисел больше 5.")
  }
}
