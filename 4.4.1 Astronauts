//Для пилотируемой миссии на Марс отобрали троих космонавтов. Беда в том, что они оказались настолько похожи, что выбрать командира экипажа оказалось затруднительно. Тогда решили проверить их IQ. Напишите программу, которая запрашивает IQ трёх космонавтов и назначает командира по наивысшему IQ.

package main

import (
	"fmt"
)

func main() {
  var iq1, iq2, iq3 int
	fmt.Print("Введите IQ первого касмонавта: ")
  fmt.Scan(&iq1)
  fmt.Print("Введите IQ второго касмонавта: ")
  fmt.Scan(&iq2)
  fmt.Print("Введите IQ третьего касмонавта: ")
  fmt.Scan(&iq3)

  if iq1 >= 0 && iq2 >= 0 && iq3 >= 0{
    if (iq1 > iq2 && iq1 > iq3 && iq2 == iq3) || (iq1 > iq2 && iq1 > iq3 && iq2 != iq3) {
      fmt.Println("Командиром будет первый космонавт.")
    } else if (iq1 > iq2 && iq1 == iq3) || (iq1 > iq3 && iq1 == iq2) {
      fmt.Println("IQ должны быть различны, иначе мы не сможем выбрать капитана")
    } else if (iq2 > iq1 && iq2 > iq3 && iq1 == iq3) || (iq2 > iq1 && iq2 > iq3 && iq1 != iq3) {
      fmt.Println("Командиром будет второй космонавт.")
    } else if (iq2 > iq1 && iq2 == iq3) || (iq2 > iq3 && iq2 == iq1) {
      fmt.Println("IQ должны быть различны, иначе мы не сможем выбрать капитана")
    } else if (iq3 > iq1 && iq3 > iq2 && iq1 == iq2) || (iq3 > iq1 && iq3 > iq2 && iq1 != iq2) {
      fmt.Println("Командиром будет третий космонавт.")
    } else if (iq3 > iq1 && iq3 == iq2) || (iq3 > iq2 && iq3 == iq1) {
      fmt.Println("IQ должны быть различны, иначе мы не сможем выбрать капитана")
    } else {
      fmt.Println("IQ должны быть различны, иначе мы не сможем выбрать капитана")
    }
  } else {
    fmt.Println("IQ не может быть меньше 0")
  }
}
